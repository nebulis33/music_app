<form action="<%= notes_path %>" method="POST">
    <input type="hidden" name="authenticity_token" data="<%= form_authenticity_token %>">
    <input type="hidden" name="note[track_id]" value="<%= track.id %>">
    <label for="body">Note Body:</label>
    <br>
    <textarea name="note[body]" id="body" cols="30" rows="10"></textarea>
    <br>
    <input type="submit" value="Post">
</form>